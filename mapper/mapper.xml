<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="test">
    <select id="selectList">
        select * from test_info
        <where>
            <if test="num != null">
                and num = #{num}
            </if>
        </where>
    </select>
</mapper>   